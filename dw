local library = loadstring(game.HttpGet(game, "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/0x"))()

local w1 = library:Window("RF24 | FUCKER")

function findBall()
    for _, part in ipairs(Workspace:GetDescendants()) do
        if part:IsA("BasePart") and part.CanCollide == true and part.Transparency == 1 and part.BrickColor == targetColor then
            return part
        end
    end
    return nil
end

local lockOnBall = false

function lockOn()
    while lockOnBall do
        local ball = findBall()
        local player = game.Players.LocalPlayer.Character
        if ball and player then
            player.HumanoidRootPart.CFrame = ball.CFrame * CFrame.new(0, 0, -5)  -- Position behind the ball
        end
        wait(0.1)  -- Wait a short time to reduce lag
    end
end

local Workspace = game:GetService("Workspace")
local targetColor = BrickColor.new(Color3.fromRGB(237, 234, 234))
local speed = 16
local fov = 70

local footSize = 1  -- Default foot size


w1:Toggle(
    "Reach",
    "FootSize",
    false,
    function(toggled)
        local rightFoot = game.Players.LocalPlayer.Character:FindFirstChild("RightFoot")
        if toggled and rightFoot then
            rightFoot.Size = Vector3.new(footSize, footSize, footSize)
            local bodyPos = Instance.new("BodyPosition")
            bodyPos.Position = rightFoot.Position
            rightFoot.BodyPosition = bodyPos
        elseif rightFoot then
            rightFoot.Size = Vector3.new(1, 1, 1)  -- Reset to original size
            if rightFoot:FindFirstChild("BodyPosition") then
                rightFoot.BodyPosition:Remove()
            end
        end
    end
)

w1:Slider(
    "Radius",
    "Size",
    1,
    10,
    function(value)
        footSize = value
    end
)


w1:Button(
    "Goto ball",
    function()
        for i,v in pairs(Workspace:GetDescendants()) do
            if v:IsA("BasePart") and v.CanCollide == true and v.Transparency == 1 and v.BrickColor == targetColor then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                break
            end
        end
    end
)

w1:Button(
    "Bring Ball (OP)",
    function()
        for i,v in pairs(Workspace:GetDescendants()) do
            if v:IsA("BasePart") and v.CanCollide == true and v.Transparency == 1 and v.BrickColor == targetColor then
                v.Anchored = true
                v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                break
            end
        end
    end
)

w1:Toggle(
    "Ball Aim (OP)",
    "LockOnBall",
    false,
    function(toggled)
        lockOnBall = toggled
        if lockOnBall then
            lockOn()
        end
    end
)

local w2 = library:Window("RF24 | MISC")

local UserInputService = game:GetService("UserInputService")
local ctrlTPEnabled = false

w2:Toggle(
    "Ctrl+TP",
    "CtrlTP",
    false,
    function(toggled)
        ctrlTPEnabled = toggled
    end
)

UserInputService.InputBegan:Connect(function(input)
    if ctrlTPEnabled and input.UserInputType == Enum.UserInputType.MouseButton1 and UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
        local mouse = game.Players.LocalPlayer:GetMouse()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(mouse.Hit.p)
    end
end)

w2:Button(
    "Instant Leave (Server)",
    function()
        game.Players.LocalPlayer:Kick("bye bye")
    end
)

w2:Button(
    "Reset Character",
    function()
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end
)


w2:Slider(
    "Speed",
    "WS",
    16,
    25,
    function(value)
        speed = value
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speed
    end
)

w2:Slider(
    "FOV",
    "FOV",
    0,
    180,
    function(value)
        fov = value
        game.Workspace.CurrentCamera.FieldOfView = fov
    end
)


print("43443")

