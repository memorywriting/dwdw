local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/0x"))()

local Workspace = game:GetService("Workspace")
local UserInputService = game:GetService("UserInputService")
local targetColor = BrickColor.new(Color3.fromRGB(237, 234, 234))
local speed = 16
local fov = 70
local footSize = 1
local ctrlTPEnabled = false

function findBall()
    for _, part in ipairs(Workspace:GetDescendants()) do
        if part:IsA("BasePart") and part.CanCollide and part.Transparency == 1 and part.BrickColor == targetColor then
            return part
        end
    end
    return nil
end

local w1 = library:Window("RF24 | FUCKER")
local w2 = library:Window("RF24 | MISC")

w1:Toggle(
    "Reach",
    "FootSize",
    false,
    function(toggled)
        local rightFoot = game.Players.LocalPlayer.Character:FindFirstChild("RightFoot")
        if toggled and rightFoot then
            rightFoot.Size = Vector3.new(footSize, footSize, footSize)
            local bodyPos = Instance.new("BodyPosition")
            bodyPos.Position = rightFoot.Position
            rightFoot.BodyPosition = bodyPos
        elseif rightFoot then
            rightFoot.Size = Vector3.new(1, 1, 1)
            if rightFoot:FindFirstChild("BodyPosition") then
                rightFoot.BodyPosition:Destroy()
            end
        end
    end
)

w1:Slider(
    "Radius",
    "Size",
    1,
    10,
    function(value)
        footSize = value
    end
)

w1:Button(
    "Goto ball",
    function()
        local ball = findBall()
        if ball then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = ball.CFrame
        end
    end
)

w1:Button(
    "Bring Ball (OP)",
    function()
        local ball = findBall()
        if ball then
            ball.Anchored = true
            ball.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        end
    end
)

w1:Toggle(
    "Bypass Votekick (shit)",
    "CtrlTP",
    false,
    function(toggled)
        local stopLoop = false
        while toggled and not stopLoop do
            table.remove(game:GetService('Players'):GetPlayers(), table.find(game:GetService('Players'):GetPlayers(), game.Players.LocalPlayer))
            if #game:GetService('Players'):GetPlayers() > 0 then
                game:GetService('Players'):Chat(":votekick " .. game:GetService('Players'):GetPlayers()[math.random(#game:GetService('Players'):GetPlayers())].Name)
            end
            task.wait(30)
        end
    end
)


local Players = game:GetService('Players')
local localPlayer = Players.LocalPlayer
local character = localPlayer.Character
local humanoid = character and character:FindFirstChildOfClass('Humanoid')

if humanoid then
    humanoid.AutoRotate = true
end

w1:Toggle(
    "Aim at Away",
    "LookAway",
    false,
    function(toggled)
        while toggled and humanoid do
            local awayPart = workspace.game.system.goal.Away
            humanoid:LookAt(awayPart.Position)
        end
    end
)

w1:Toggle(
    "Aim at Home",
    "LookHome",
    false,
    function(toggled)
        while toggled and humanoid do
            local homePart = workspace.game.system.goal.Home
            humanoid:LookAt(homePart.Position)
        end
    end
)

w2:Toggle(
    "Ctrl TP",
    "CtrlTP",
    false,
    function(toggled)
        ctrlTPEnabled = toggled
    end
)

UserInputService.InputBegan:Connect(function(input)
    if ctrlTPEnabled and input.UserInputType == Enum.UserInputType.MouseButton1 and UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players.LocalPlayer:GetMouse().Hit.p)
    end
end)

w2:Button(
    "Instant Leave (Server)",
    function()
        game.Players.LocalPlayer:Kick("bye bye")
    end
)

w2:Button(
    "Reset Character",
    function()
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end
)

w2:Button(
    "Refresh Character",
    function()
        game:GetService('Players'):Chat("rtp")
    end
)

w2:Slider(
    "Speed",
    "WS",
    16,
    25,
    function(value)
        speed = value
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speed
    end
)

w2:Slider(
    "FOV",
    "FOV",
    0,
    180,
    function(value)
        fov = value
        game.Workspace.CurrentCamera.FieldOfView = fov
    end
)

print("43443")
